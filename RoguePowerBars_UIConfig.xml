<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">

	<CheckButton name="CheckBoxTemplate" inherits="OptionsCheckButtonTemplate" virtual="true">
		<HitRectInsets>
			<AbsInset left="0" right="0" top="0" bottom="0"/>
		</HitRectInsets>
		<Scripts>
			<OnClick>
				if ( this:GetChecked() ) then
					PlaySound("igMainMenuOptionCheckBoxOff");
				else
					PlaySound("igMainMenuOptionCheckBoxOn");
				end
			</OnClick>
		</Scripts>
	</CheckButton>
	
	<Frame virtual="true" name="NormalCheckBoxTemplate">
		<Size>
			<AbsDimension x="200" y="24"/>
		</Size>
		<Frames>
			<CheckButton name="$parent_CheckBox" inherits="CheckBoxTemplate">
    		    <Size>
          			<AbsDimension x="24" y="24"/>
        		</Size>
				<Anchors>
					<Anchor point="BOTTOMLEFT" relativeTo="$parent" relativePoint="BOTTOMLEFT">
						<Offset>
							<AbsDimension x="0" y="16"/>
						</Offset>
					</Anchor>
				</Anchors>
			</CheckButton>

			<Frame>
				<Layers>
					<Layer level="OVERLAY">
						<FontString name="$parent_DescribeText" inherits="GameFontNormalSmall">
							<Anchors>
								<Anchor point="LEFT">
									<Offset>
										<AbsDimension x="32" y="1"/>
									</Offset>
								</Anchor>
							</Anchors>
						</FontString>
					</Layer>
				</Layers>
				<Anchors>
					<Anchor point="TOPLEFT" relativeTo="$parent_CheckBox" relativePoint="TOPLEFT" />
					<Anchor point="BOTTOMRIGHT" relativeTo="$parent_CheckBox" relativePoint="BOTTOMRIGHT" />
				</Anchors>
			</Frame>
		</Frames>
	</Frame>
	
	<Frame name="RoguePowerBar_GUIFrame">
		<Scripts>
			<OnLoad>
				RoguePowerBar_GUIFrame_OnLoad(self);
			</OnLoad>
		</Scripts>
	</Frame>
</Ui>	